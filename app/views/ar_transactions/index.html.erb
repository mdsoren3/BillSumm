<h1>Listing ar_transactions</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Transactionamount</th>
      <th>Transactiontype</th>
      <th>Note</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @ar_transactions.each do |ar_transaction| %>
      <tr>
        <td><%= ar_transaction.date %></td>
        <td><%= ar_transaction.transactionAmount %></td>
        <td><%= ar_transaction.transactionType %></td>
        <td><%= ar_transaction.note %></td>
        <td><%= link_to 'Show', ar_transaction %></td>
        <td><%= link_to 'Edit', edit_ar_transaction_path(ar_transaction) %></td>
        <td><%= link_to 'Destroy', ar_transaction, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Ar transaction', new_ar_transaction_path %>
